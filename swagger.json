{
  "paths": {
    "/books": {
      "get": {
        "responses": {
          "201": {
            "description": "Retrieve all books"
          }
        }
      }
    },
    "/books/{bookId}": {
      "get": {
        "responses": {
          "200": {
            "description": "Retrieve details of a specific book"
          }
        }
      },
      "delete": {
        "responses": {
          "204": {
            "description": "Delete a specific book"
          }
        }
      }
    }
  }
}

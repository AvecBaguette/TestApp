{
  "paths": {
    "/books": {
      "get": {
        "responses": {
          "200": {
            "description": "Retrieve all books"
          }
        }
      },
      "post": {
        "responses": {
          "201": {
            "description": "Add a new book"
          }
        }
      }
    },
    "/books/{bookId}": {
      "get": {
        "responses": {
          "200": {
            "description": "Retrieve details of a specific book"
          },
          "404": {
            "description": "Book not found"
          }
        }
      }
    }
  }
}
